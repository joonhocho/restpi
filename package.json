{
  "name": "restpi",
  "version": "0.0.1",
  "description": "REST API client for various third party cloud services",
  "main": "es5/index.js",
  "types": "es5/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run format && npm run lint:fix && npm run test && npm run build && npm run build:es5",
    "build": "tsc -p ./tsconfig.json && tscpaths -p ./tsconfig.json -s ./src -o ./lib",
    "build:es5": "tsc -p ./tsconfig.es5.json && tscpaths -p ./tsconfig.es5.json -s ./src -o ./es5",
    "clean": "rm -rf ./lib ./es5 ./coverage",
    "format": "prettier --write \"./*.{js,jsx,ts,tsx}\" \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "tslint -c ./tslint.json \"src/**/*\"",
    "lint:fix": "tslint --fix -c ./tslint.json \"src/**/*\"",
    "precommit": "npm run all",
    "prepublish": "npm run all",
    "reinstall": "rm -rf ./node_modules package-lock.json && npm i",
    "start": "npm run test",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch"
  },
  "pre-commit": "precommit",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joonhocho/restpi.git"
  },
  "keywords": [
    "REST",
    "API client",
    "cloud services",
    "library",
    "TypeScript"
  ],
  "author": "Joon Ho Cho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joonhocho/restpi/issues"
  },
  "homepage": "https://github.com/joonhocho/restpi#readme",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "tsapis": "0.0.3",
    "tscpaths": "^0.0.9",
    "tsdef": "^0.0.7",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
